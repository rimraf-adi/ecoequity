generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  
}

model SolBackup {
  id Int @id @default(autoincrement())
  curr_price Float
  bids Json[]
  asks Json[]
}

model TimeSeriesSol {
  id Int @id @default(autoincrement())
  current_price  Float
  time String
}