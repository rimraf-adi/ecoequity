generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  
}

model backup {
  id Int @id @default(autoincrement())
  market String
  curr_price Float
  bids Json[]
  asks Json[]

}
model ohlc {
  id Int @unique @default(autoincrement())
  open Float
  high Float
  low Float
  close Float
  timestamp Int
  market String
}